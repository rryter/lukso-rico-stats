{
  "version": 1,
  "projects": {
    "rtm": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/rtm",
      "sourceRoot": "apps/rtm/src",
      "prefix": "lukso",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/rtm",
            "index": "apps/rtm/src/index.html",
            "main": "apps/rtm/src/main.ts",
            "polyfills": "apps/rtm/src/polyfills.ts",
            "tsConfig": "apps/rtm/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/rtm/src/favicon.ico", "apps/rtm/src/assets", "apps/rtm/src/.htaccess"],
            "styles": ["apps/rtm/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/rtm/src/environments/environment.ts",
                  "with": "apps/rtm/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "ci": {
              "fileReplacements": [
                {
                  "replace": "apps/rtm/src/app/shared/services/web3/web3.provider.ts",
                  "with": "apps/rtm/src/app/shared/services/web3/web3.provider.ci.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "rtm:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "rtm:build:production"
            },
            "ci": {
              "browserTarget": "rtm:build:ci"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "rtm:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/rtm/tsconfig.app.json", "apps/rtm/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/rtm/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/rtm/jest.config.js",
            "tsConfig": "apps/rtm/tsconfig.spec.json",
            "passWithNoTests": false
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {}
        }
      }
    },
    "rtm-e2e": {
      "root": "apps/rtm-e2e",
      "sourceRoot": "apps/rtm-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/rtm-e2e/cypress.json",
            "tsConfig": "apps/rtm-e2e/tsconfig.e2e.json",
            "devServerTarget": "rtm:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "rtm:serve:production"
            },
            "ci": {
              "devServerTarget": "rtm:serve:ci"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/rtm-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/rtm-e2e/**/*"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "rtm"
}
